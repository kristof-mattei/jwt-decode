    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'jwt-decode'",
            "cargo": {
                "args": ["build", "--bin=jwt-decode", "--package=jwt-decode"],
                "filter": {
                    "name": "jwt-decode",
                    "kind": "bin"
                }
            },
            "args": ["<PUT JWT HERE>"],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "full",
                "RUST_LOG": "jwt_decode=trace"
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'jwt-decode'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=jwt-decode",
                    "--package=jwt-decode"
                ],
                "filter": {
                    "name": "jwt-decode",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "RUST_BACKTRACE": "full",
                "RUST_LOG": "jwt_decode=trace"
            }
        }
    ]
}
